 *
 */
	public String difference(long company, long organ, long place, String sDate, String cDate) throws Exception {
	// 실행에 사용될 변수
		DBLibrary dbLib = new DBLibrary();
		Connection conn = null;
		PreparedStatement ps = null;
		ResultSet rs = null;
		String error = null;

	// DB 연결
		conn = dbLib.getConnection(this.cfg.get("db.jdbc.name"), this.cfg.get("db.jdbc.host"), this.cfg.get("db.jdbc.user"), this.cfg.get("db.jdbc.pass"));

		if (conn == null) {
			return "DB 연결에 실패하였습니다.";
		}

	// 판매 내역
		this.list = new ArrayList<GeneralConfig>();

		try {
			ps = conn.prepareStatement("SELECT /*+ ORDERED USE_NL(B C D) INDEX_DESC(A UK_SALES_2) */ TO_CHAR(TO_DATE(A.TRANSACTION_DATE || A.TRANSACTION_TIME, 'YYYYMMDDHH24MISS'), 'YYYY-MM-DD HH24:MI:SS') AS TRANSACTION_DATE, A.AMOUNT, A.COMMISSION, CASE WHEN A.PAY_TYPE = '11' THEN D.NAME ELSE B.NAME END AS PAY_TYPE, C.NAME AS PAY_STEP, CASE WHEN TO_DATE(A.PAY_DATE, 'YYYYMMDD') > TO_DATE(A.PAY_DATE_EXP, 'YYYYMMDD') THEN '후입금' WHEN TO_DATE(A.PAY_DATE, 'YYYYMMDD') < TO_DATE(A.PAY_DATE_EXP, 'YYYYMMDD') THEN '선입금' ELSE '-' END AS REASON"
				+ " FROM TB_SALES A LEFT JOIN TB_CODE B ON (A.PAY_TYPE = B.CODE AND B.TYPE = 'PAY_TYPE') LEFT JOIN TB_CODE C ON (A.PAY_STEP = C.CODE AND C.TYPE = 'PAY_STEP') LEFT JOIN TB_CODE D ON (A.PAY_CARD = D.CODE AND D.TYPE = 'PAY_CARD')"
				+ " WHERE A.PAY_TYPE IN ('01', '11') AND A.PAY_STEP IN ('03') AND COMPANY_SEQ = ? AND ORGANIZATION_SEQ = ? AND VM_PLACE_SEQ = ? AND CLOSING_DATE = ? AND PAY_DATE = ?");
			ps.setLong(1, company);
			ps.setLong(2, organ);
			ps.setLong(3, place);
			ps.setString(4, cDate);
			ps.setString(5, sDate);
			rs = ps.executeQuery();

			while (rs.next()) {
				GeneralConfig c = new GeneralConfig();

				c.put("TRANSACTION_DATE", rs.getString("TRANSACTION_DATE"));
				c.put("AMOUNT", rs.getLong("AMOUNT"));
				c.put("COMMISSION", rs.getLong("COMMISSION"));
				c.put("PAY_TYPE", rs.getString("PAY_TYPE"));
				c.put("PAY_STEP", rs.getString("PAY_STEP"));
				c.put("REASON", rs.getString("REASON"));

				this.list.add(c);
			}
		} catch (Exception e) {
			error = this.cfg.error(this, e);
		} finally {
			try {
				if (rs != null) {
					rs.close();
					rs = null;
				}
			} catch (Exception e_) {
			}

			try {
				if (ps != null) {
					ps.close();
					ps = null;
				}
			} catch (Exception e_) {
			}
		}

	// 에러 처리
		if (!StringEx.isEmpty(error)) {
			try {
				if (conn != null) {
					conn.close();
					conn = null;
				}
			} catch (Exception e) {
			}

			return error;
		}

	// 리소스 반환
		try {
			if (conn != null) {
				conn.close();
				conn = null;
			}
		} catch (Exception e) {
		}

		return null;
