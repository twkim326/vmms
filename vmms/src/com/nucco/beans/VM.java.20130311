				error = "소속이 존재하지 않습니다.";
			} else if (excel == null || excel.length() <= 0) {
				error = "등록하실 엑셀을 업로드하세요.";
			} else if (!StringEx.inArray(FileEx.extension(excel.getName()), "xls".split(";"))) {
				error = "XLS 파일만 등록이 가능합니다.";
			}

			if (!StringEx.isEmpty(error)) {
				if (excel.exists()) {
					excel.delete();
				}

				return error;
			}

